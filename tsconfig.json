{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": ["ES2020", "DOM"],
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true
	}
}
